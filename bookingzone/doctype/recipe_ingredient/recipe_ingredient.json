{
  "name": "Recipe Ingredient",
  "module": "BookingZone",
  "doctype": "DocType",
  "document_type": "Document",
  "istable": 1,
  "engine": "InnoDB",
  "field_order": [
    "item_code",
    "item_name",
    "quantity",
    "uom",
    "cost",
    "notes"
  ],
  "fields": [
    {
      "fieldname": "item_code",
      "fieldtype": "Link",
      "label": "Item Code",
      "options": "Item",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "item_name",
      "fieldtype": "Data",
      "label": "Item Name",
      "fetch_from": "item_code.item_name",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "quantity",
      "fieldtype": "Float",
      "label": "Quantity",
      "reqd": 1,
      "in_list_view": 1,
      "default": 1
    },
    {
      "fieldname": "uom",
      "fieldtype": "Link",
      "label": "UOM",
      "options": "UOM",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "cost",
      "fieldtype": "Currency",
      "label": "Cost",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    }
  ],
  "track_changes": 1
}
