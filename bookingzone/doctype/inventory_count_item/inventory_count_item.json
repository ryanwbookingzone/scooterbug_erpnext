{
  "name": "Inventory Count Item",
  "module": "BookingZone",
  "doctype": "DocType",
  "document_type": "Document",
  "istable": 1,
  "engine": "InnoDB",
  "field_order": [
    "item_code",
    "item_name",
    "uom",
    "system_qty",
    "counted_qty",
    "variance_qty",
    "system_value",
    "counted_value",
    "variance_value",
    "notes"
  ],
  "fields": [
    {
      "fieldname": "item_code",
      "fieldtype": "Link",
      "label": "Item Code",
      "options": "Item",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "item_name",
      "fieldtype": "Data",
      "label": "Item Name",
      "fetch_from": "item_code.item_name",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "uom",
      "fieldtype": "Link",
      "label": "UOM",
      "options": "UOM",
      "fetch_from": "item_code.stock_uom",
      "in_list_view": 1
    },
    {
      "fieldname": "system_qty",
      "fieldtype": "Float",
      "label": "System Qty",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "counted_qty",
      "fieldtype": "Float",
      "label": "Counted Qty",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "variance_qty",
      "fieldtype": "Float",
      "label": "Variance Qty",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "system_value",
      "fieldtype": "Currency",
      "label": "System Value",
      "read_only": 1
    },
    {
      "fieldname": "counted_value",
      "fieldtype": "Currency",
      "label": "Counted Value",
      "read_only": 1
    },
    {
      "fieldname": "variance_value",
      "fieldtype": "Currency",
      "label": "Variance Value",
      "read_only": 1
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    }
  ],
  "track_changes": 1
}
