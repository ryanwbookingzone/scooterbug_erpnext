{
    "name": "Expense Receipt",
    "module": "Accounts",
    "doctype": "DocType",
    "document_type": "Document",
    "engine": "InnoDB",
    "field_order": [
        "receipt_details_section",
        "receipt_date",
        "vendor",
        "column_break_1",
        "amount",
        "category",
        "receipt_image_section",
        "receipt_image",
        "ocr_section",
        "ocr_extracted_data",
        "ocr_confidence",
        "ocr_engine",
        "column_break_2",
        "ocr_status",
        "ocr_processed_at",
        "accounting_section",
        "expense_account",
        "cost_center",
        "column_break_3",
        "payment_method",
        "tax_amount",
        "additional_info_section",
        "notes",
        "status",
        "journal_entry"
    ],
    "fields": [
        {
            "fieldname": "receipt_details_section",
            "fieldtype": "Section Break",
            "label": "Receipt Details"
        },
        {
            "fieldname": "receipt_date",
            "fieldtype": "Date",
            "label": "Receipt Date",
            "reqd": 1,
            "default": "Today"
        },
        {
            "fieldname": "vendor",
            "fieldtype": "Link",
            "label": "Vendor",
            "options": "Supplier"
        },
        {
            "fieldname": "column_break_1",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "amount",
            "fieldtype": "Currency",
            "label": "Amount",
            "reqd": 1
        },
        {
            "fieldname": "category",
            "fieldtype": "Link",
            "label": "Expense Category",
            "options": "Item Group"
        },
        {
            "fieldname": "receipt_image_section",
            "fieldtype": "Section Break",
            "label": "Receipt Image"
        },
        {
            "fieldname": "receipt_image",
            "fieldtype": "Attach Image",
            "label": "Receipt Image"
        },
        {
            "fieldname": "ocr_section",
            "fieldtype": "Section Break",
            "label": "OCR Processing",
            "collapsible": 1
        },
        {
            "fieldname": "ocr_extracted_data",
            "fieldtype": "JSON",
            "label": "OCR Extracted Data",
            "read_only": 1
        },
        {
            "fieldname": "ocr_confidence",
            "fieldtype": "Percent",
            "label": "OCR Confidence",
            "read_only": 1
        },
        {
            "fieldname": "ocr_engine",
            "fieldtype": "Data",
            "label": "OCR Engine",
            "read_only": 1,
            "default": "PaddleOCR"
        },
        {
            "fieldname": "column_break_2",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "ocr_status",
            "fieldtype": "Select",
            "label": "OCR Status",
            "options": "\nPending\nProcessing\nCompleted\nFailed",
            "read_only": 1
        },
        {
            "fieldname": "ocr_processed_at",
            "fieldtype": "Datetime",
            "label": "OCR Processed At",
            "read_only": 1
        },
        {
            "fieldname": "accounting_section",
            "fieldtype": "Section Break",
            "label": "Accounting"
        },
        {
            "fieldname": "expense_account",
            "fieldtype": "Link",
            "label": "Expense Account",
            "options": "Account"
        },
        {
            "fieldname": "cost_center",
            "fieldtype": "Link",
            "label": "Cost Center",
            "options": "Cost Center"
        },
        {
            "fieldname": "column_break_3",
            "fieldtype": "Column Break"
        },
        {
            "fieldname": "payment_method",
            "fieldtype": "Select",
            "label": "Payment Method",
            "options": "\nCash\nCredit Card\nDebit Card\nBank Transfer\nCheck"
        },
        {
            "fieldname": "tax_amount",
            "fieldtype": "Currency",
            "label": "Tax Amount"
        },
        {
            "fieldname": "additional_info_section",
            "fieldtype": "Section Break",
            "label": "Additional Information"
        },
        {
            "fieldname": "notes",
            "fieldtype": "Small Text",
            "label": "Notes"
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "Draft\nPending Review\nApproved\nRejected\nPosted",
            "default": "Draft"
        },
        {
            "fieldname": "journal_entry",
            "fieldtype": "Link",
            "label": "Journal Entry",
            "options": "Journal Entry",
            "read_only": 1
        }
    ],
    "permissions": [
        {
            "role": "Accounts User",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 0,
            "submit": 0,
            "cancel": 0
        },
        {
            "role": "Accounts Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1,
            "submit": 1,
            "cancel": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
    "title_field": "vendor",
    "autoname": "naming_series:EXP-REC-.YYYY.-.#####",
    "naming_rule": "By \"Naming Series\" field"
}
