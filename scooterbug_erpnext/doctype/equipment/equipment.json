{
  "name": "Equipment",
  "module": "Scooterbug Erpnext",
  "doctype": "DocType",
  "document_type": "Document",
  "engine": "InnoDB",
  "field_order": [
    "equipment_id",
    "equipment_name",
    "equipment_type",
    "column_break_1",
    "category",
    "location",
    "status",
    "section_break_2",
    "serial_number",
    "manufacturer",
    "model",
    "column_break_3",
    "purchase_date",
    "warranty_expiry",
    "purchase_price",
    "section_break_4",
    "weight_lbs",
    "weight_capacity_lbs",
    "battery_life_hours",
    "column_break_5",
    "max_speed_mph",
    "turning_radius_inches",
    "section_break_6",
    "features",
    "description",
    "image",
    "section_break_7",
    "daily_rate",
    "weekly_rate",
    "damage_waiver_rate",
    "section_break_8",
    "current_battery_level",
    "last_maintenance_date",
    "next_maintenance_date",
    "total_rental_days",
    "column_break_9",
    "gps_latitude",
    "gps_longitude",
    "last_gps_update",
    "section_break_10",
    "condition_notes",
    "is_available"
  ],
  "fields": [
    {
      "fieldname": "equipment_id",
      "fieldtype": "Data",
      "label": "Equipment ID",
      "reqd": 1,
      "unique": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "equipment_name",
      "fieldtype": "Data",
      "label": "Equipment Name",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1
    },
    {
      "fieldname": "equipment_type",
      "fieldtype": "Select",
      "label": "Equipment Type",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "ECV Scooter\nStandard Scooter\nLightweight Scooter\nStandard Wheelchair\nTransport Wheelchair\nBariatric Wheelchair\nSingle Stroller\nDouble Stroller\nRollator Walker\nKnee Scooter"
    },
    {
      "fieldname": "column_break_1",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "category",
      "fieldtype": "Select",
      "label": "Category",
      "reqd": 1,
      "options": "ECV Scooter\nWheelchair\nStroller\nWalker"
    },
    {
      "fieldname": "location",
      "fieldtype": "Link",
      "label": "Location",
      "options": "ScooterBug Location",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "Available\nRented\nMaintenance\nReserved\nRetired",
      "default": "Available"
    },
    {
      "fieldname": "section_break_2",
      "fieldtype": "Section Break",
      "label": "Equipment Details"
    },
    {
      "fieldname": "serial_number",
      "fieldtype": "Data",
      "label": "Serial Number",
      "unique": 1
    },
    {
      "fieldname": "manufacturer",
      "fieldtype": "Data",
      "label": "Manufacturer"
    },
    {
      "fieldname": "model",
      "fieldtype": "Data",
      "label": "Model"
    },
    {
      "fieldname": "column_break_3",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "purchase_date",
      "fieldtype": "Date",
      "label": "Purchase Date"
    },
    {
      "fieldname": "warranty_expiry",
      "fieldtype": "Date",
      "label": "Warranty Expiry"
    },
    {
      "fieldname": "purchase_price",
      "fieldtype": "Currency",
      "label": "Purchase Price"
    },
    {
      "fieldname": "section_break_4",
      "fieldtype": "Section Break",
      "label": "Specifications"
    },
    {
      "fieldname": "weight_lbs",
      "fieldtype": "Float",
      "label": "Weight (lbs)"
    },
    {
      "fieldname": "weight_capacity_lbs",
      "fieldtype": "Float",
      "label": "Weight Capacity (lbs)"
    },
    {
      "fieldname": "battery_life_hours",
      "fieldtype": "Float",
      "label": "Battery Life (hours)"
    },
    {
      "fieldname": "column_break_5",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "max_speed_mph",
      "fieldtype": "Float",
      "label": "Max Speed (mph)"
    },
    {
      "fieldname": "turning_radius_inches",
      "fieldtype": "Float",
      "label": "Turning Radius (inches)"
    },
    {
      "fieldname": "section_break_6",
      "fieldtype": "Section Break",
      "label": "Description & Features"
    },
    {
      "fieldname": "features",
      "fieldtype": "Small Text",
      "label": "Features"
    },
    {
      "fieldname": "description",
      "fieldtype": "Text Editor",
      "label": "Description"
    },
    {
      "fieldname": "image",
      "fieldtype": "Attach Image",
      "label": "Image"
    },
    {
      "fieldname": "section_break_7",
      "fieldtype": "Section Break",
      "label": "Pricing"
    },
    {
      "fieldname": "daily_rate",
      "fieldtype": "Currency",
      "label": "Daily Rate",
      "reqd": 1
    },
    {
      "fieldname": "weekly_rate",
      "fieldtype": "Currency",
      "label": "Weekly Rate"
    },
    {
      "fieldname": "damage_waiver_rate",
      "fieldtype": "Currency",
      "label": "Damage Waiver Rate (per day)",
      "default": 6
    },
    {
      "fieldname": "section_break_8",
      "fieldtype": "Section Break",
      "label": "Current Status"
    },
    {
      "fieldname": "current_battery_level",
      "fieldtype": "Percent",
      "label": "Current Battery Level"
    },
    {
      "fieldname": "last_maintenance_date",
      "fieldtype": "Date",
      "label": "Last Maintenance Date"
    },
    {
      "fieldname": "next_maintenance_date",
      "fieldtype": "Date",
      "label": "Next Maintenance Date"
    },
    {
      "fieldname": "total_rental_days",
      "fieldtype": "Int",
      "label": "Total Rental Days",
      "read_only": 1
    },
    {
      "fieldname": "column_break_9",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "gps_latitude",
      "fieldtype": "Float",
      "label": "GPS Latitude",
      "precision": 6
    },
    {
      "fieldname": "gps_longitude",
      "fieldtype": "Float",
      "label": "GPS Longitude",
      "precision": 6
    },
    {
      "fieldname": "last_gps_update",
      "fieldtype": "Datetime",
      "label": "Last GPS Update"
    },
    {
      "fieldname": "section_break_10",
      "fieldtype": "Section Break",
      "label": "Notes"
    },
    {
      "fieldname": "condition_notes",
      "fieldtype": "Small Text",
      "label": "Condition Notes"
    },
    {
      "fieldname": "is_available",
      "fieldtype": "Check",
      "label": "Is Available",
      "default": 1
    }
  ],
  "autoname": "field:equipment_id",
  "naming_rule": "By fieldname",
  "title_field": "equipment_name",
  "image_field": "image",
  "search_fields": "equipment_name,equipment_type,serial_number,location",
  "sort_field": "modified",
  "sort_order": "DESC",
  "track_changes": 1,
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Sales User",
      "read": 1,
      "write": 1
    }
  ]
}
