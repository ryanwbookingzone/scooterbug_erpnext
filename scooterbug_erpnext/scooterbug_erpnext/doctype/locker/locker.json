{
  "name": "Locker",
  "module": "scooterbug_erpnext",
  "doctype": "DocType",
  "document_type": "Document",
  "engine": "InnoDB",
  "field_order": [
    "locker_id",
    "locker_name",
    "locker_size",
    "column_break_1",
    "location",
    "park_name",
    "park_area",
    "section_break_2",
    "status",
    "is_available",
    "column_break_3",
    "daily_rate",
    "section_break_4",
    "dimensions",
    "description",
    "image"
  ],
  "fields": [
    {
      "fieldname": "locker_id",
      "fieldtype": "Data",
      "label": "Locker ID",
      "reqd": 1,
      "unique": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "locker_name",
      "fieldtype": "Data",
      "label": "Locker Name",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "locker_size",
      "fieldtype": "Select",
      "label": "Locker Size",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "Small\nMedium\nLarge\nJumbo"
    },
    {
      "fieldname": "column_break_1",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "location",
      "fieldtype": "Link",
      "label": "Location",
      "options": "ScooterBug Location",
      "reqd": 1,
      "in_standard_filter": 1
    },
    {
      "fieldname": "park_name",
      "fieldtype": "Data",
      "label": "Park Name",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "park_area",
      "fieldtype": "Data",
      "label": "Park Area"
    },
    {
      "fieldname": "section_break_2",
      "fieldtype": "Section Break",
      "label": "Availability"
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "options": "Available\nRented\nMaintenance\nOut of Service",
      "default": "Available",
      "in_list_view": 1
    },
    {
      "fieldname": "is_available",
      "fieldtype": "Check",
      "label": "Is Available",
      "default": 1
    },
    {
      "fieldname": "column_break_3",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "daily_rate",
      "fieldtype": "Currency",
      "label": "Daily Rate",
      "reqd": 1
    },
    {
      "fieldname": "section_break_4",
      "fieldtype": "Section Break",
      "label": "Details"
    },
    {
      "fieldname": "dimensions",
      "fieldtype": "Data",
      "label": "Dimensions"
    },
    {
      "fieldname": "description",
      "fieldtype": "Small Text",
      "label": "Description"
    },
    {
      "fieldname": "image",
      "fieldtype": "Attach Image",
      "label": "Image"
    }
  ],
  "autoname": "field:locker_id",
  "naming_rule": "By fieldname",
  "title_field": "locker_name",
  "image_field": "image",
  "search_fields": "locker_name,park_name,locker_size",
  "sort_field": "modified",
  "sort_order": "DESC",
  "track_changes": 1,
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Sales User",
      "read": 1,
      "write": 1
    }
  ]
}
