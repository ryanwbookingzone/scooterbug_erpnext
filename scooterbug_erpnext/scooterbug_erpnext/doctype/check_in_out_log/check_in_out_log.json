{
  "name": "Check In Out Log",
  "module": "scooterbug_erpnext",
  "doctype": "DocType",
  "document_type": "Document",
  "engine": "InnoDB",
  "field_order": [
    "log_type",
    "booking",
    "equipment",
    "column_break_1",
    "timestamp",
    "processed_by",
    "location",
    "section_break_2",
    "customer_name",
    "customer_signature",
    "column_break_3",
    "equipment_condition",
    "battery_level",
    "section_break_4",
    "condition_notes",
    "damage_reported",
    "damage_description",
    "section_break_5",
    "photo_before",
    "photo_after"
  ],
  "fields": [
    {
      "fieldname": "log_type",
      "fieldtype": "Select",
      "label": "Log Type",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "Check-In\nCheck-Out"
    },
    {
      "fieldname": "booking",
      "fieldtype": "Link",
      "label": "Booking",
      "options": "Equipment Booking",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "equipment",
      "fieldtype": "Link",
      "label": "Equipment",
      "options": "Equipment",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "column_break_1",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "timestamp",
      "fieldtype": "Datetime",
      "label": "Timestamp",
      "reqd": 1,
      "in_list_view": 1,
      "default": "Now"
    },
    {
      "fieldname": "processed_by",
      "fieldtype": "Link",
      "label": "Processed By",
      "options": "User"
    },
    {
      "fieldname": "location",
      "fieldtype": "Link",
      "label": "Location",
      "options": "ScooterBug Location"
    },
    {
      "fieldname": "section_break_2",
      "fieldtype": "Section Break",
      "label": "Customer Information"
    },
    {
      "fieldname": "customer_name",
      "fieldtype": "Data",
      "label": "Customer Name"
    },
    {
      "fieldname": "customer_signature",
      "fieldtype": "Signature",
      "label": "Customer Signature"
    },
    {
      "fieldname": "column_break_3",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "equipment_condition",
      "fieldtype": "Select",
      "label": "Equipment Condition",
      "options": "Excellent\nGood\nFair\nPoor\nDamaged",
      "default": "Good"
    },
    {
      "fieldname": "battery_level",
      "fieldtype": "Percent",
      "label": "Battery Level"
    },
    {
      "fieldname": "section_break_4",
      "fieldtype": "Section Break",
      "label": "Condition Notes"
    },
    {
      "fieldname": "condition_notes",
      "fieldtype": "Small Text",
      "label": "Condition Notes"
    },
    {
      "fieldname": "damage_reported",
      "fieldtype": "Check",
      "label": "Damage Reported"
    },
    {
      "fieldname": "damage_description",
      "fieldtype": "Small Text",
      "label": "Damage Description",
      "depends_on": "damage_reported"
    },
    {
      "fieldname": "section_break_5",
      "fieldtype": "Section Break",
      "label": "Photos"
    },
    {
      "fieldname": "photo_before",
      "fieldtype": "Attach Image",
      "label": "Photo (Before)"
    },
    {
      "fieldname": "photo_after",
      "fieldtype": "Attach Image",
      "label": "Photo (After)"
    }
  ],
  "autoname": "naming_series:",
  "naming_rule": "By \"Naming Series\" field",
  "naming_series_options": "CIO-.YYYY.-.#####",
  "title_field": "booking",
  "search_fields": "booking,equipment,customer_name",
  "sort_field": "timestamp",
  "sort_order": "DESC",
  "track_changes": 1,
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Sales User",
      "read": 1,
      "write": 1,
      "create": 1
    }
  ]
}
