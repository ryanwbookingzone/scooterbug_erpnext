{
  "name": "Travel Agent",
  "module": "scooterbug_erpnext",
  "doctype": "DocType",
  "document_type": "Document",
  "engine": "InnoDB",
  "field_order": [
    "agent_id",
    "agent_name",
    "company_name",
    "column_break_1",
    "status",
    "tier",
    "section_break_2",
    "email",
    "phone",
    "website",
    "column_break_3",
    "address",
    "city",
    "state",
    "zip_code",
    "section_break_4",
    "commission_rate",
    "total_bookings",
    "total_commission_earned",
    "column_break_5",
    "pending_commission",
    "last_booking_date",
    "section_break_6",
    "notes",
    "contract_start_date",
    "contract_end_date"
  ],
  "fields": [
    {
      "fieldname": "agent_id",
      "fieldtype": "Data",
      "label": "Agent ID",
      "reqd": 1,
      "unique": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "agent_name",
      "fieldtype": "Data",
      "label": "Agent Name",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "company_name",
      "fieldtype": "Data",
      "label": "Company Name",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "column_break_1",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "status",
      "fieldtype": "Select",
      "label": "Status",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "Pending\nActive\nInactive\nSuspended",
      "default": "Pending"
    },
    {
      "fieldname": "tier",
      "fieldtype": "Select",
      "label": "Tier",
      "in_list_view": 1,
      "in_standard_filter": 1,
      "options": "Bronze\nSilver\nGold\nPlatinum",
      "default": "Bronze"
    },
    {
      "fieldname": "section_break_2",
      "fieldtype": "Section Break",
      "label": "Contact Information"
    },
    {
      "fieldname": "email",
      "fieldtype": "Data",
      "label": "Email",
      "options": "Email",
      "reqd": 1
    },
    {
      "fieldname": "phone",
      "fieldtype": "Data",
      "label": "Phone"
    },
    {
      "fieldname": "website",
      "fieldtype": "Data",
      "label": "Website"
    },
    {
      "fieldname": "column_break_3",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "address",
      "fieldtype": "Small Text",
      "label": "Address"
    },
    {
      "fieldname": "city",
      "fieldtype": "Data",
      "label": "City"
    },
    {
      "fieldname": "state",
      "fieldtype": "Data",
      "label": "State"
    },
    {
      "fieldname": "zip_code",
      "fieldtype": "Data",
      "label": "Zip Code"
    },
    {
      "fieldname": "section_break_4",
      "fieldtype": "Section Break",
      "label": "Commission & Performance"
    },
    {
      "fieldname": "commission_rate",
      "fieldtype": "Percent",
      "label": "Commission Rate",
      "default": 10
    },
    {
      "fieldname": "total_bookings",
      "fieldtype": "Int",
      "label": "Total Bookings",
      "read_only": 1
    },
    {
      "fieldname": "total_commission_earned",
      "fieldtype": "Currency",
      "label": "Total Commission Earned",
      "read_only": 1
    },
    {
      "fieldname": "column_break_5",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "pending_commission",
      "fieldtype": "Currency",
      "label": "Pending Commission",
      "read_only": 1
    },
    {
      "fieldname": "last_booking_date",
      "fieldtype": "Date",
      "label": "Last Booking Date",
      "read_only": 1
    },
    {
      "fieldname": "section_break_6",
      "fieldtype": "Section Break",
      "label": "Contract"
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    },
    {
      "fieldname": "contract_start_date",
      "fieldtype": "Date",
      "label": "Contract Start Date"
    },
    {
      "fieldname": "contract_end_date",
      "fieldtype": "Date",
      "label": "Contract End Date"
    }
  ],
  "autoname": "field:agent_id",
  "naming_rule": "By fieldname",
  "title_field": "agent_name",
  "search_fields": "agent_name,company_name,email",
  "sort_field": "modified",
  "sort_order": "DESC",
  "track_changes": 1,
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Sales User",
      "read": 1
    }
  ]
}
