{
  "name": "ScooterBug Location",
  "module": "ScooterBug",
  "doctype": "DocType",
  "document_type": "Setup",
  "engine": "InnoDB",
  "field_order": [
    "location_name",
    "location_code",
    "column_break_1",
    "city",
    "state",
    "section_break_2",
    "address",
    "phone",
    "email",
    "column_break_3",
    "timezone",
    "is_active",
    "section_break_4",
    "operating_hours_start",
    "operating_hours_end",
    "column_break_5",
    "delivery_radius_miles",
    "base_delivery_fee"
  ],
  "fields": [
    {
      "fieldname": "location_name",
      "fieldtype": "Data",
      "label": "Location Name",
      "reqd": 1,
      "in_list_view": 1,
      "in_standard_filter": 1
    },
    {
      "fieldname": "location_code",
      "fieldtype": "Data",
      "label": "Location Code",
      "reqd": 1,
      "unique": 1
    },
    {
      "fieldname": "column_break_1",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "city",
      "fieldtype": "Data",
      "label": "City",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "state",
      "fieldtype": "Data",
      "label": "State",
      "reqd": 1
    },
    {
      "fieldname": "section_break_2",
      "fieldtype": "Section Break",
      "label": "Contact Information"
    },
    {
      "fieldname": "address",
      "fieldtype": "Small Text",
      "label": "Address"
    },
    {
      "fieldname": "phone",
      "fieldtype": "Data",
      "label": "Phone"
    },
    {
      "fieldname": "email",
      "fieldtype": "Data",
      "label": "Email",
      "options": "Email"
    },
    {
      "fieldname": "column_break_3",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "timezone",
      "fieldtype": "Select",
      "label": "Timezone",
      "options": "America/New_York\nAmerica/Los_Angeles\nAmerica/Chicago\nAmerica/Denver"
    },
    {
      "fieldname": "is_active",
      "fieldtype": "Check",
      "label": "Is Active",
      "default": 1
    },
    {
      "fieldname": "section_break_4",
      "fieldtype": "Section Break",
      "label": "Operations"
    },
    {
      "fieldname": "operating_hours_start",
      "fieldtype": "Time",
      "label": "Operating Hours Start",
      "default": "07:00:00"
    },
    {
      "fieldname": "operating_hours_end",
      "fieldtype": "Time",
      "label": "Operating Hours End",
      "default": "22:00:00"
    },
    {
      "fieldname": "column_break_5",
      "fieldtype": "Column Break"
    },
    {
      "fieldname": "delivery_radius_miles",
      "fieldtype": "Int",
      "label": "Delivery Radius (Miles)",
      "default": 30
    },
    {
      "fieldname": "base_delivery_fee",
      "fieldtype": "Currency",
      "label": "Base Delivery Fee",
      "default": 0
    }
  ],
  "autoname": "field:location_code",
  "naming_rule": "By fieldname",
  "title_field": "location_name",
  "search_fields": "location_name,city,state",
  "sort_field": "location_name",
  "sort_order": "ASC",
  "track_changes": 1,
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Sales Manager",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Sales User",
      "read": 1
    }
  ]
}
