{
  "name": "SB Booking",
  "module": "scooterbug",
  "autoname": "naming_series:",
  "naming_rule": "",
  "sort_field": "modified",
  "sort_order": "DESC",
  "is_submittable": 1,
  "istable": 0,
  "editable_grid": 1,
  "track_changes": 1,
  "track_views": 0,
  "allow_rename": 1,
  "allow_copy": 0,
  "allow_import": 0,
  "quick_entry": 0,
  "show_name_in_global_search": 0,
  "engine": "InnoDB",
  "doctype": "DocType",
  "fields": [
    {
      "fieldname": "naming_series",
      "label": "Series",
      "fieldtype": "Select",
      "options": "BK-.YYYY.-",
      "default": "BK-.YYYY.-",
      "hidden": 1
    },
    {
      "fieldname": "customer",
      "label": "Customer",
      "fieldtype": "Link",
      "options": "SB Customer",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "location",
      "label": "Location",
      "fieldtype": "Link",
      "options": "SB Location",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "booking_date",
      "label": "Booking Date",
      "fieldtype": "Date",
      "reqd": 1,
      "default": "Today"
    },
    {
      "fieldname": "start_date",
      "label": "Start Date",
      "fieldtype": "Date",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "end_date",
      "label": "End Date",
      "fieldtype": "Date",
      "reqd": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "status",
      "label": "Status",
      "fieldtype": "Select",
      "options": "Draft\nConfirmed\nIn Progress\nCompleted\nCancelled",
      "default": "Draft",
      "in_list_view": 1
    },
    {
      "fieldname": "section_items",
      "label": "Booking Items",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "items",
      "label": "Items",
      "fieldtype": "Table",
      "options": "SB Booking Item",
      "reqd": 1
    },
    {
      "fieldname": "section_totals",
      "label": "Totals",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "subtotal",
      "label": "Subtotal",
      "fieldtype": "Currency",
      "read_only": 1
    },
    {
      "fieldname": "tax_amount",
      "label": "Tax Amount",
      "fieldtype": "Currency"
    },
    {
      "fieldname": "discount_amount",
      "label": "Discount Amount",
      "fieldtype": "Currency"
    },
    {
      "fieldname": "total_amount",
      "label": "Total Amount",
      "fieldtype": "Currency",
      "in_list_view": 1,
      "read_only": 1
    },
    {
      "fieldname": "section_payment",
      "label": "Payment",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "payment_status",
      "label": "Payment Status",
      "fieldtype": "Select",
      "options": "Pending\nPartial\nPaid\nRefunded",
      "default": "Pending"
    },
    {
      "fieldname": "payment_method",
      "label": "Payment Method",
      "fieldtype": "Select",
      "options": "Credit Card\nDebit Card\nCash\nCheck"
    },
    {
      "fieldname": "amount_paid",
      "label": "Amount Paid",
      "fieldtype": "Currency"
    },
    {
      "fieldname": "section_notes",
      "label": "Notes",
      "fieldtype": "Section Break"
    },
    {
      "fieldname": "special_requests",
      "label": "Special Requests",
      "fieldtype": "Text"
    },
    {
      "fieldname": "internal_notes",
      "label": "Internal Notes",
      "fieldtype": "Text"
    },
    {
      "fieldname": "amended_from",
      "label": "Amended From",
      "fieldtype": "Link",
      "options": "SB Booking",
      "read_only": 1,
      "no_copy": 1,
      "print_hide": 1,
      "search_index": 1
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1,
      "submit": 1,
      "cancel": 1,
      "report": 1,
      "export": 1,
      "share": 1,
      "print": 1,
      "email": 1
    }
  ],
  "links": [],
  "actions": [],
  "states": []
}